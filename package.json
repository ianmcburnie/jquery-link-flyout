{
  "name": "jquery-link-flyout",
  "description": "jQuery collection plugin that converts a link + div, into a link + hidden button + flyout and handles all hide/show behaviour",
  "version": "0.4.3",
  "scripts": {
    "start": "karma start --autoWatch true --singleRun false",
    "test": "karma start",
    "prepublish": "npm run build",
    "lint": "jshint jquery.linkflyout.js > jshint.txt",
    "minify": "jsmin -o jquery.linkflyout.min.js jquery.linkflyout.js",
    "clean": "rimraf test_reports *.min.js jshint.txt",
    "build": "npm run clean && npm run lint && npm test && npm run minify"
  },
  "author": "Ian McBurnie <ianmcburnie@hotmail.com>",
  "homepage": "https://github.com/ianmcburnie/jquery-link-flyout",
  "bugs": {
    "url": "https://github.com/ianmcburnie/jquery-link-flyout/issues"
  },
  "keywords": [
    "jquery-plugin",
    "ecosystem:jquery",
    "ebayui",
    "accessibility",
    "a11y"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ianmcburnie/jquery-link-flyout.git"
  },
  "license": "ISC",
  "devDependencies": {
    "coveralls": "^2.11.4",
    "jasmine-core": "^2.3.4",
    "jquery": ">=1.11 < 3",
    "jshint": "^2.8.0",
    "jsmin": "^1.0.1",
    "karma": "^0.13.9",
    "karma-coverage": "^0.5.0",
    "karma-html-reporter": "^0.2.6",
    "karma-jasmine": "^0.3.6",
    "karma-junit-reporter": "^0.3.3",
    "karma-phantomjs-launcher": "^0.2.1",
    "phantomjs": "^1.9.18",
    "rimraf": "^2.4.4",
    "jquery-mouse-exit": "~0.1.0",
    "jquery-button-flyout": "~0.5.0"
  },
  "peerDependencies": {
    "jquery": ">=1.11 < 3",
    "jquery-mouse-exit": "~0.1.0",
    "jquery-button-flyout": "~0.5.0"
  }
}
